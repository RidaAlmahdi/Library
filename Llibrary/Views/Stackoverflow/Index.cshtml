<div class="container c-body" style="">
    <div class="d-flex">
        <div class="align-self-center mx-auto">
            <h2>Stack Overflow Last 50 Questions</h2>
            <table id="stackOverflowQuestionsTable" class="table table-striped table-bordered" style="width:100%">

                <thead>
                    <tr>
                        <th>Id</th>
                        <th>Owner</th>
                        <th>Title</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var question in ViewBag.stackOverflowQuestions)
                    {
                        <tr>
                            <td>@question.question_id</td>
                            <td>@question.owner.display_name</td>
                            <td class="btn-link" style="cursor:pointer"  onclick="window.location.href = '/StackoverflowQuestionById/@question.question_id'">@question.title</td>

                        </tr>
                    }
                </tbody>
            </table>
        </div>

    </div>
</div>
<script>
    $(function () {

        $('#stackOverflowQuestionsTable').createTablePagination();

    });

</script>