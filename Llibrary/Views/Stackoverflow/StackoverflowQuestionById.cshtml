<div class="container c-body" style="">
    <div class="d-flex">
        <div class="align-self-center mx-auto">
            <h2>Stack Overflow Question Details</h2>
            <table id="stackOverflowQuestionTable" class="table table-striped table-bordered" style="width:100%">

                <thead>
                    <tr>
                        <th>Id</th>
                        <th>Owner</th>
                        <th>Title</th>
                        <th>Link</th>
                        <th>Is Answered</th>
                        <th>View Count</th>
                        <th>Answer Count</th>
                        <th>Score</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var question in ViewBag.stackOverflowQuestion)
                    {
                        <tr>
                            <td>@question.question_id</td>
                            <td>@question.owner.display_name</td>
                            <td>@question.title</td>
                            <td><a href="@question.link" target="_blank"> @question.link</a></td>
                            <td>@question.is_answered</td>
                            <td>@question.view_count</td>
                            <td>@question.answer_count</td>
                            <td>@question.score</td>

                        </tr>
                    }
                </tbody>
            </table>
        </div>

    </div>
</div>
<script>
    $(function () {

        $('#stackOverflowQuestionTable').createTablePagination();

    });

</script>